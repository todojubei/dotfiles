!  Xft Settings

Xft.dpi:         96
Xft.antialias:   true
Xft.rgba:        rgb
Xft.hinting:     true
Xft.hintstyle:   hintslight

! URxvt Settings

URxvt.depth:            32
URxvt.geometry:         90x30
URxvt.loginShell:       true
URxvt.internalBorder:   8
URxvt.lineSpace:        0

URxvt.saveLines: 300
URxvt.scrollBar: false

URxvt.font: xft: GohuFont: pixelsize: 14

URxvt.perl-ext:
URxvt.perl-ext-common:

